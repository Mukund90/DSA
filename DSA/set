#include<iostream>
#include<set>
using namespace std;
int main() 
{
   set<int , greater<int>>s1{90,78,67,55};

   cout<<" The size of the set is ->"<<" "<<s1.size();

   cout<<endl;

   for(auto i: s1)
   {
    cout<<i<<" ";

   }

   cout<<endl;

   cout<<"The element is present or not-> "<<" "<<s1.count(67);

   cout<<endl;

   s1.erase(90);

   for(auto i: s1)
   {
    cout<<i<<" ";
   }

   


   return 0;


}
