#include<iostream>
#include<queue>
using namespace std;

int main()
{
    queue<string>q1;

    q1.push("mukund");
    q1.push("kumar");
    q1.push("jha");

    cout<<" The front element of the queue is ->"<<q1.front()<<endl;

    cout<<"The size of the queue is "<<q1.size()<<endl;

    q1.pop();

    cout<<" After popping the elements in the queue is ->"<<q1.front()<<endl;

    cout<<"The size of the queue is ->"<<q1.size()<<endl;

    cout<<endl;
    cout<<" Is it empty->"<<q1.empty()<<endl;

    cout<<endl;
    q1.pop();
    cout<<"The size of The queue is ->"<<q1.size();
    cout<<" The front element after popping->"<<q1.front()<<endl;
}
